.background
  background-position: center
  background-repeat: no-repeat
  background-attachment: fixed
  background-size: cover
  position: relative
  width: 100%
  height: 100%

.id1 .background
  background-image: url(images-directory + 'frontPage/beach.jpg')

.id2 .background
  background-image: url(images-directory + 'aloha.jpg')

.id3 .background
  background-image: url(images-directory + 'muir.jpg')
  position: inherit

.id4 .background
  background-image: url(images-directory + 'breckenridge.jpg')

.id5 .background
  background-image: url(images-directory + 'skulls.png')
  background-repeat: repeat
  background-size: auto

.id5
  position: relative

.front-page-pictures
  background-size: cover
  width: 100%
  height: 500px
  display: inline-block
  white-space: nowrap
  border: 1px solid #000

.frontpage
  background-size: cover
  width: 300px
  height: 100%
  display:inline-block

.beach
  background-image: url(images-directory + 'frontPage/beach.jpg')
  background-position-x: -100px

.colorado
  background-image: url(images-directory + 'frontPage/colorado.jpg')
  background-position-x: -350px
  background-size: 700px
.disneyland
  background-image: url(images-directory + 'frontPage/disneyland.jpg')
  background-position-x: -150px
  background-size: 700px
.firecracker
  background-image: url(images-directory + 'frontPage/firecracker.jpg')
  background-position-x: -222px
  background-size: 800px
.santa
  background-image: url(images-directory + 'frontPage/santa.jpg')
.seattle
  background-image: url(images-directory + 'frontPage/seattle.jpg')
  
.backImg.small-width
  width: 100%
  height: auto
